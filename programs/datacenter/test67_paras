#用项目名为前缀命名的参数为项目共用参数
datacenter_apollo==FAT
datacenter_addr==172.16.118.63
datacenter_start==1
datacenter_mem=='-Xmx512m -Xms512m -Xmn256m'

#用应用名命名的参数为应用指定参数
datacenter-controller_tomcat==/data/tomcat-datacenter-7010
datacenter-controller_port==[7010]
#项目关联，多个项目关联请用英文逗号分隔
data-center-facade_rele==data-center-test67/datacenter+data-center-service+dev-20200409guohaifeng+test67+package